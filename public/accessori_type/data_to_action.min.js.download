decodeBase64=function(t){var j={},h,n=0,m,p,g=0,o,d="",q=String.fromCharCode,k=t.length;var f="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/";for(h=0;h<64;h++){j[f.charAt(h)]=h}for(p=0;p<k;p++){m=j[t.charAt(p)];n=(n<<6)+m;g+=6;while(g>=8){((o=(n>>>(g-=8))&255)||(p<(k-2)))&&(d+=q(o))}}return d};data_to_action=function(a){$(a).click(function(){document.location=decodeBase64($(a).attr("data-to"))})};